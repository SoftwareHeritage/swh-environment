[swh-alter]
checkout = git clone 'https://gitlab.softwareheritage.org/swh/devel/swh-alter.git' 'swh-alter'
fixups =
  git remote set-url origin https://gitlab.softwareheritage.org/swh/devel/swh-alter.git
  grep -q pre-commit.com .git/hooks/pre-commit || pre-commit install
  grep -q pre-commit.com .git/hooks/commit-msg || pre-commit install -t commit-msg

